{
    "name": "backfron/laravel-finder",
    "description": "Build and handle complex Eloquent searchs easely.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jairo Ushi√±a",
            "email": "jago86@gmail.com",
            "homepage": "https://backfron.com"
        }
    ],
    "homepage": "https://github.com/backfron/laravel-finder",
    "keywords": ["Laravel", "LaravelFinder"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Backfron\\LaravelFinder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backfron\\LaravelFinder\\Tests\\": "tests",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backfron\\LaravelFinder\\LaravelFinderServiceProvider"
            ],
            "aliases": {
                "LaravelFinder": "Backfron\\LaravelFinder\\Facades\\LaravelFinder"
            }
        }
    }
}
